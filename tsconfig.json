{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/middleware.ts",
    "src/@core/apps/context/main.jsx",
    "src/@core/shared/hooks/useAuthContext.jsx",
    "src/@core/shared/hooks/useAuth.jsx",
    "src/@core/shared/UI/ExcelTable/index.tsx",
    "src/@core/apps/utils/fn.js",
    "src/@core/pages/Dashboard/model/Model.jsx",
    "src/@core/shared/ui/Autocomplete/index.tsx"
  ],
  "exclude": ["node_modules"]
}
